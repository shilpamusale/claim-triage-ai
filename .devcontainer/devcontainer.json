{
    "name": "Claim Triage AI Codespace",
    "image": "mcr.microsoft.com/devcontainers/python:1-3.11-bullseye",
    "features": {
        "ghcr.io/devcontainers/features/docker-in-docker:2": {
            "moby": false
        }
    },
    "postCreateCommand": "curl -sSL https://sdk.cloud.google.com | bash -s -- --disable-prompts && echo 'source $HOME/google-cloud-sdk/path.bash.inc' >> $HOME/.bashrc",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance"
            ]
        }
    }
}